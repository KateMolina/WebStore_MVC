@model IEnumerable<WebStore_MVC.Models.Employee>
@{
    ViewData["Title"] = "Employees";
}
<h1>@ViewData["Title"]</h1>

<hr />
<table>
    <thead>
        <tr>
            <th>#</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age</th>
            
        </tr>
    </thead>
    <tbody>
        
        @foreach (var employee in Model)
        {
          <tr>
            <td>@employee.Id</td>
            <td>@employee.FirstName</td>
            <td>@employee.LastName</td>
            <td>@employee.Age</td>
         </tr>
        }
    
    </tbody>
</table>
<p>
    <a href="/">Main Page</a>
</p>